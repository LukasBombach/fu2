%h2 Sent messages
=render :partial => "navigation"
%ul#message_list
  -@outgoing_messages.each do |message|
    %li
      .messages_who
        =h(message.sender_display_name)
      .messages_date
        =message.created_at.strftime('%Y/%m/%d %H:%M:%S')
      .messages_subject
        =link_to h(message.subject), message_path(message)
      .clear